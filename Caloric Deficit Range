#Weight in Kg, Height in Cm
weight <- 113
height <- 186
age <- 26

#Total Caloric Requirement and exercise Co-Factor (Mifflin - St. Jeor Equation)
TCR <- (9.99*weight) + (6.25*height) - (4.92*age) - 161
CoFctr <- 1.375

#Calories/day with Upper and Lower Limit
CalDay <- TCR*CoFctr
UpLmt <- CalDay - 250
LowLmt <- CalDay - 550

#Caloric Deficit Range
CalRng <- range(UpLmt,LowLmt)
CalRng

#Protein Intake in grams
PrtInt <- weight * 1.2
PrtInt
